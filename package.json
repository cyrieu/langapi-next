{
  "name": "langapi-next",
  "version": "1.1.6",
  "description": "NextJS support for langapi",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run format",
    "preversion": "npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently --kill-others \"tsc -w\" \"nodemon dist/index.js\"",
    "build": "tsc && cp src/select/styles.css dist/select",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cyrieu/langapi-next.git"
  },
  "author": "Eric Yu <cyrieu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cyrieu/langapi-next/issues"
  },
  "homepage": "https://github.com/cyrieu/langapi-next#readme",
  "devDependencies": {
    "@types/hoist-non-react-statics": "^3.0.1",
    "@types/next": "^8.0.1",
    "@types/node": "^11.9.4",
    "@types/react": "^16.8.3",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.3",
    "react": ">= 16.8.0",
    "react-dom": "^16.8.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@emotion/styled": "^10.0.12",
    "hoist-non-react-statics": "^3.3.0",
    "langapi-client": "^1.4.8",
    "nookies": "^2.0.6",
    "react-cookie": "^4.0.0"
  },
  "peerDependencies": {
    "react": ">= 16.8.0"
  }
}
